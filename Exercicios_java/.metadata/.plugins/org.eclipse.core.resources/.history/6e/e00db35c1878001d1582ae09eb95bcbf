package fundamentos;

public class NotacaoPonto {

	public static void main(String[] args) {

		double a = 2.3;
		String s = "Bom dia meus fi";
		
		// toUpperCase deixa todas as letras maiusculas naquela determinada linha, para armazenar esse novo valor vc deve declara
		s = s.toUpperCase();
		System.out.println(s);
		
		// toUpperCase deixa todas as letras maiusculas naquela determinada linha, para armazenar esse novo valor vc deve declara
		s = s.replace("fi","Pessoal");
		System.out.println(s);
	}

}
